@using T1808A_MVC.Models
@model IEnumerable<T1808A_MVC.Models.Student>

@{
    ViewBag.Title = "Index";
    List<Category> listCategory = ViewData["Categories"] as List<Category>;
}

<ul>
    
    
</ul>
<h2>Index @ViewData["categoryId"]</h2>
<form action="/Students" method="get">
    <input type="text" name="keyword"/>
    <input type="submit" value="Search" />
</form>
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.RollNumber)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.RollNumber)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
                @Html.ActionLink("Details", "Details", new { id=item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.Id })
            </td>
        </tr>
    }

</table>

